#include <iostream>
using namespace std; 
int main()
{
    int num1, num2;
    cout << " Input the first integer: ";
    cin >> num1;
    cout << " Input the second integer: ";
    cin >> num2;
    if (num1 == num2)
        cout << num1 << " is equal to " << num2 << endl;
    if (num1 != num2)
        cout << num1 << " not equal to " << num2 << endl;
}
